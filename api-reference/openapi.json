  "components": {
    "schemas": {
      "InstantApplyRequest": {
        "type": "object",
        "required": [
          "initial_code",
          "edit_snippet"
        ],
        "properties": {
          "initial_code": {
            "type": "string",
            "description": "The original code that needs to be modified"
          },
          "edit_snippet": {
            "type": "string",
            "description": "The code changes to be applied to the initial code"
          },
          "stream": {
            "type": "boolean",
            "description": "Whether to stream the response back",
            "default": false
          },
